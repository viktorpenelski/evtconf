<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.viktorpenelski.evtconf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.github.viktorpenelski.evtconf.EvtConfApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.viktorpenelski.evtconf.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.viktorpenelski.evtconf.WebViewActivity" />
    </application>

</manifest>